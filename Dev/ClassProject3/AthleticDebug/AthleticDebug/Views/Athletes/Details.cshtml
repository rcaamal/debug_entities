@model IEnumerable<AthleticDebug.DAL.Result>

@{
    ViewBag.Title = "Details";
    string temp = ViewBag.Picture;
}

<h2>Details</h2>
<img src=@temp alt="Image Not Available" width="250" height="300">
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Athlete.FName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TimeEvents)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distance_Miles)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SwimResult)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Athlete.FName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeEvents)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distance_Miles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SwimResult)
            </td>

        </tr>
    }

</table>
@Html.ActionLink("Back to List", "Index", "Athletes")


